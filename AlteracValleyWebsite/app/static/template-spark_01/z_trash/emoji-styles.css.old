body {
  font-family: 'Lato', sans-serif;
  font-size: 1.1rem;
  font-weight: 400;
  line-height: 1.6;
  color: #333;
  font-smooth: auto;
}

.tooltip, .popover, .btn, .btn.m-btn--custom, .m-portlet.m-portlet--creative .m-portlet__head .m-portlet__head-caption .m-portlet__head-title .m-portlet__head-text, .vakata-context,
.vakata-context ul {
    font-family: 'Lato', sans-serif;
}


.aside-line {
  border-top: 1px solid #242533;
  height: auto;
}






.emoji-boxplot-wrapper {
  position: relative;
  display: block;
  width: 100%;
  margin: 30px auto 40px;
}
.popovers {
  height: 50px;
  vertical-align: bottom;
}
.m-badge {
    font-size: .95rem;
    margin-bottom: 10px;
}
.m-badge.m-badge--metal {
    background-color: #666;
    color: #fff
}
.emoji-boxplot-column-one {
  float: left;
  position: relative;
  display: block;
  width: 100%;
  margin: 0 auto;
  text-align: center;
}
.emoji-boxplot-column-two, .emoji-boxplot-column-three {
  width: 40%;
  margin-top: 16px;
  font-size: .85rem;
  color: #333;
  text-transform: uppercase;
}
.highlight {
  font-weight: bold;
}
.emoji-boxplot-column-two {
  float: left;
  text-align: left;
  margin-right: 15px;
}
.emoji-boxplot-column-three {
  float: right;
  text-align: right;
  margin-left: 15px;
}

.emoji-boxplot-background {
  background: #c5ecf6;
  width: 100%;
  height: 20px;
}
.emoji-boxplot-foreground {
  background: #0096BB;
  height: 100%;
  border-left: 1px solid #fff;
  border-right: 1px solid #fff;
}

.percent-average {
  border-right: 2px solid #444;
  position: absolute;
  margin-left: -1px;
  height: 28px;
  bottom: -4px;
}
.percent-average>span {
  position: absolute;
  bottom: 40px;
  left: -10px;
  font-size: .9rem;
  line-height: 0;
  overflow: visible;
  color: #333;
  font-weight: bold;
}
.emoji-boxplot-percentages {
  width: 100%;
  position: absolute;
  margin-top: 9px;
  font-size: .85rem;
  line-height: 0;
  overflow: visible;
  color: #0096BB;
}
.emoji-boxplot-percentages .percent-low {
  position: absolute;
  margin-left: 0px;
  text-align: right;
}
.emoji-boxplot-percentages .percent-high {
  position: absolute;
}


